{
  "name": "angular2-showtime",
  "version": "0.0.1",
  "description": "Project description",
  "keywords": [
    "angular-2-generator",
    " angular-2-generator",
    " angular2",
    " etereo",
    " corbel",
    " angular2",
    " etereo"
  ],
  "author": {},
  "repository": {
    "type": "git"
  },
  "private": true,
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --config build/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && node_modules/.bin/ngc -p tsconfig.aot.json && webpack --config build/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "ci": "npm run lint && npm test && npm run test:e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --open --config build/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "karma start",
    "test:unit:debug": "karma start --no-single-run",
    "test:unit": "karma start",
    "test:e2e": "npm run protractor",
    "test:e2e:live": "npm run e2e -- --elementExplorer",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "webtranslateit:update": "node ./tools/webtranslateit.js"
  },
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@etereo/auth": "^0.6.15",
    "@etereo/corbel": "^0.6.1",
    "@etereo/core": "^0.4.0",
    "@etereo/dom-utils": "^0.1.0",
    "@etereo/http": "^0.4.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.22",
    "@ngrx/core": "1.2.0",
    "@types/google-maps": "^3.2.0",
    "@types/lodash": "4.14.59",
    "@types/socket.io-client": "^1.4.29",
    "bootstrap": "^4.0.0-alpha.6",
    "core-decorators": "^0.15.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "ng2-toastr": "4.0.1",
    "ng2-translate": "^5.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.3.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "typescript": "^2.2.2",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/compiler": "4.0.1",
    "@angular/compiler-cli": "4.0.1",
    "@angular/platform-server": "4.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.2.34",
    "@types/jquery": "^2.0.34",
    "@types/node": "^6.0.38",
    "@types/protractor": "^1.5.20",
    "@types/selenium-webdriver": "2.44.29",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "angular2-template-loader": "0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.5",
    "awesome-typescript-loader": "3.0.7",
    "bootstrap-loader": "2.0.0-beta.21",
    "codelyzer": "^2.0.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "html-webpack-plugin": "^2.28.0",
    "jsonfile": "^2.4.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.1",
    "lodash": "^4.17.4",
    "node-sass": "^3.11.1",
    "parse5": "^2.2.2",
    "postcss-load-config": "1.1.0",
    "postcss-loader": "1.3.1",
    "protractor": "^4.0.9",
    "raw-loader": "0.5.1",
    "reflect-metadata": "^0.1.8",
    "resolve-url-loader": "2.0.0",
    "rimraf": "2.5.2",
    "sass-loader": "6.0.2",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.2",
    "ts-node": "1.3.0",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.0",
    "typescript": "2.1.6",
    "underscore-deep-extend": "1.1.5",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-link": "^0.2.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "3.0.0"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3"
  }
}
